{
  "name": "pomegranate",
  "version": "7.0.0-beta.13",
  "description": "Super flexible whitebox web application framework",
  "author": "Jim Bulkowski <jim.b@paperelectron.com> (http://paperelectron.com)",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "cli/**/*",
    "index.d.ts",
    "index.js.map",
    "Framework/**/*",
    "Readme.md",
    "LICENCE"
  ],
  "scripts": {
    "prepublishOnly": "npm run script:updateVersionFile && tsc",
    "script:updateVersionFile": "./__scripts/updateTsVersionFile.js",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc --watch",
    "build:mocks": "tsc -p tsconfig-mocks.json && tsc -p tsconfig-mocks.json -d --emitDeclarationOnly --allowJs false",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:js": "jest --config=./jest.js-config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pomegranate/Pomegranate.git"
  },
  "keywords": [
    "Framework",
    "Plugins",
    "IoC"
  ],
  "bugs": {
    "url": "https://github.com/Pomegranate/Pomegranate/issues"
  },
  "homepage": "https://github.com/Pomegranate/Pomegranate",
  "bin": {
    "pomegranate": "./cli/pomegranate.js"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/stringify-object": "^3.2.0",
    "@types/yargs": "^12.0.9",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@pomegranate/plugin-tools": "^2.0.7",
    "bluebird": "^3.5.1",
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.1.0",
    "immutable-dll": "^3.0.0",
    "lodash": "^4.17.11",
    "lodash-fun": "^1.7.0",
    "magnum-di": "^6.0.0",
    "monet": "^0.9.0",
    "stringify-object": "^3.3.0",
    "uuid": "^3.3.2",
    "yargs": "^13.2.1"
  }
}
