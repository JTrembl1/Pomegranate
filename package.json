{
  "name": "pomegranate",
  "version": "2.0.0",
  "description": "An unassuming framework for building web applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaperElectron/Pomegranate.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --ui tdd --reporter spec",
    "readme": "jsdoc2md -t docs/readme.hbs \"lib/**/*.js\" > Readme.md",
    "jekyll": "cd _gh-pages && bundle exec jekyll serve --baseurl ''",
    "buildDocs": "./buildDocs.sh",
    "publish": "git subtree push --prefix _gh-pages origin gh-pages"
  },
  "keywords": [
    "Framework",
    "SQL",
    "Couch",
    "Redis"
  ],
  "author": "Jim <jim.b@paperelectron.com> (http://paperelectron.com/)",
  "license": "MIT",
  "files": [
    "index.js",
    "lib/",
    "assets/",
    "Readme.md"
  ],
  "dependencies": {
    "bluebird": "^2.9.25",
    "body-parser": "^1.12.3",
    "chalk": "^1.1.0",
    "compression": "^1.4.3",
    "connect-redis": "^2.3.0",
    "express": "^4.12.3",
    "express-session": "^1.11.1",
    "hbs": "^3.0.1",
    "jade": "^1.9.2",
    "lodash": "^3.9.1",
    "magnum-di": "^1.0.0",
    "morgan": "^1.6.0",
    "nano": "^6.1.2",
    "sequelize": "^3.13.0",
    "parent-require": "^1.0.0",
    "path-parse": "^1.0.5",
    "response-time": "^2.3.0",
    "serve-favicon": "^2.2.0",
    "serve-static": "^1.9.2",
    "yeoman-environment": "^1.2.7"
  },
  "devDependencies": {
    "gulp": "^3.8.11",
    "jsdoc-to-markdown": "^1.1.1",
    "mocha": "^2.2.4",
    "should": "^6.0.1",
    "supertest": "^1.0.1"
  }
}
